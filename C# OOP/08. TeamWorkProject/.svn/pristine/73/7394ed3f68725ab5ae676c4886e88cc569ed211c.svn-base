<Window x:Class="ESchoolDiary.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         Title="StudentWindow" Height="759" Width="1047" WindowStartupLocation="CenterScreen" >
    <Grid Background="#FF04244A" Margin="0,0,2,1">
        <Border BorderBrush="#FFD8D5D5" BorderThickness="1" HorizontalAlignment="Left" Height="587" Margin="46,99,0,0" VerticalAlignment="Top" Width="939" Background="#FFD8D5D5" CornerRadius="15"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="62,10,0,0" VerticalAlignment="Top" Width="100" Source="Images/logo.png"/>
        <Label x:Name="lblSchoolName" Content="Your School Name" HorizontalAlignment="Left" Margin="167,24,0,0" VerticalAlignment="Top" Width="190" Height="42" Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="18"/>
        <Label x:Name="lblStudentName" Content="Firstname Lastname, Student" HorizontalAlignment="Left" Margin="673,10,0,0" VerticalAlignment="Top" Width="237" Background="Transparent" Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="16"/>
        <Button Content="Logout" BorderBrush="#FF04244A" HorizontalAlignment="Left" Margin="910,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Background="#FF04244A" Foreground="#FFD8D5D5" BorderThickness="0" Cursor="Hand" FontFamily="Sylfaen" FontSize="16"/>
    </Grid>
</Window>
