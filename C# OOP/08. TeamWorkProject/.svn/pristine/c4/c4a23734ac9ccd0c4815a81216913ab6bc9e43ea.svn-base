<Window x:Class="ESchoolDiary.RegistrationalFormStudent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegistrationalFormStudent" Height="879" Width="763" WindowStartupLocation="CenterScreen" >
    <Grid Background="#FF04244A" Margin="0,0,0,0" Height="848" VerticalAlignment="Top">
        <Image HorizontalAlignment="Left" Height="100" Margin="62,10,0,0" VerticalAlignment="Top" Width="100" Source="Images/logo.png"/>
        <Label x:Name="lblSchoolName" Content="Your School Name" HorizontalAlignment="Left" Margin="167,24,0,0" VerticalAlignment="Top" Width="190" Height="42" 
    		Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="18"/>
        <Label x:Name="lblStudentName" Content="Firstname Lastname, Director" Margin="0,10,127,0" VerticalAlignment="Top" Background="Transparent" 
    		Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="16" HorizontalAlignment="Right" Width="237"/>
        <Button Content="Logout" BorderBrush="#FF04244A" Margin="0,10,52,0" VerticalAlignment="Top" Click="Button_Click_1" Background="#FF04244A" 
    		Foreground="#FFD8D5D5" BorderThickness="0" Cursor="Hand" FontFamily="Sylfaen" FontSize="16" HorizontalAlignment="Right" Width="75"/>
        <Label x:Name="lblFirstName" Content="First Name: " HorizontalAlignment="Left" Margin="185,162,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxFirstName" HorizontalAlignment="Left" Height="27" Margin="346,167,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" 
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblMiddleName" Content="Middle Name: " HorizontalAlignment="Left" Margin="185,203,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxMiddleName" HorizontalAlignment="Left" Height="27" Margin="346,208,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" 
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblLastName" Content="Last Name: " HorizontalAlignment="Left" Margin="185,239,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxLastName" HorizontalAlignment="Left" Height="27" Margin="346,248,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblGender" Content="Gender: " HorizontalAlignment="Left" Margin="185,280,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <RadioButton Content="Male" HorizontalAlignment="Left" Margin="346,284,0,0" VerticalAlignment="Top" Width="72" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5"/>
        <RadioButton Content="Female" HorizontalAlignment="Left" Margin="423,284,0,0" VerticalAlignment="Top" Width="87" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblBirthDate" Content="Birth Date: " HorizontalAlignment="Left" Margin="185,316,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <DatePicker HorizontalAlignment="Left" Margin="346,324,0,0" VerticalAlignment="Top" Width="151"/>
        <Label x:Name="lblPin" Content="Pin: " HorizontalAlignment="Left" Margin="185,352,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxPin" HorizontalAlignment="Left" Height="27" Margin="346,361,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblContacts" Content="Contacts: " HorizontalAlignment="Left" Margin="185,405,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblPhoneNumber" Content="Phone Number: " HorizontalAlignment="Left" Margin="185,442,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCountryCode" HorizontalAlignment="Left" Height="27" Margin="346,448,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="151"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"  Text="08XXXXXXXXX"/>
        <Label x:Name="lblEmail" Content="E-mail: " HorizontalAlignment="Left" Margin="185,478,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="27" Margin="346,487,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"  Text="email@example.com"/>
        <Label x:Name="lblAddress" Content="Address: " HorizontalAlignment="Left" Margin="185,542,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCountry" HorizontalAlignment="Left" Margin="346,587,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Label x:Name="lblCountry" Content="Country: " HorizontalAlignment="Left" Margin="185,578,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblCity" Content="City: " HorizontalAlignment="Left" Margin="185,614,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblStreet" Content="Street: " HorizontalAlignment="Left" Margin="185,650,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCity" HorizontalAlignment="Left" Margin="346,623,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxStreet" HorizontalAlignment="Left" Margin="346,659,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Label x:Name="lblPersonalData" Content="Personal Data: " HorizontalAlignment="Left" Margin="185,121,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblSchoolClass" Content="School Class: " HorizontalAlignment="Left" Margin="185,712,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxSchoolClass" HorizontalAlignment="Left" Margin="346,721,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Button x:Name="btnCancel" Content="Cancel" Margin="423,783,207,0" VerticalAlignment="Top" Height="32" FontFamily="Sylfaen" FontSize="18" Foreground="#FF172949" Click="btnCancel_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.502"/>
                    <GradientStop Color="#FF04244A" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Left" Margin="220,783,0,0" VerticalAlignment="Top" Width="137" Height="32" FontFamily="Sylfaen" FontSize="18" Foreground="#FF172949" Click="btnSave_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FF04244A" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
    </Grid>
</Window>
