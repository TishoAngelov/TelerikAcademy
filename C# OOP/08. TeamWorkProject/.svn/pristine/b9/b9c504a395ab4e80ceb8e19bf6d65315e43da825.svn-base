<Window x:Class="ESchoolDiary.RegistrationalFormTeacher"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegistrationalFormTeacher"  Height="1019" Width="763" WindowStartupLocation="CenterScreen" >
    <Grid Background="#FF04244A" Margin="0,0,0,0" Height="988" VerticalAlignment="Top">
        <Image HorizontalAlignment="Left" Height="100" Margin="62,10,0,0" VerticalAlignment="Top" Width="100" Source="Images/logo.png"/>
        <Label x:Name="lblSchoolName" Content="Your School Name" HorizontalAlignment="Left" Margin="167,24,0,0" VerticalAlignment="Top" Width="190" Height="42" 
    		Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="18"/>
        <Label x:Name="lblStudentName" Content="Firstname Lastname, Director" Margin="0,10,127,0" VerticalAlignment="Top" Background="Transparent" 
    		Foreground="#FFD8D5D5" FontFamily="Sylfaen" FontSize="16" HorizontalAlignment="Right" Width="237"/>
        <Button Content="Logout" BorderBrush="#FF04244A" Margin="0,10,52,0" VerticalAlignment="Top" Click="Button_Click_1" Background="#FF04244A" 
    		Foreground="#FFD8D5D5" BorderThickness="0" Cursor="Hand" FontFamily="Sylfaen" FontSize="16" HorizontalAlignment="Right" Width="75"/>
        <Label x:Name="lblFirstName" Content="First Name: " HorizontalAlignment="Left" Margin="179,158,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxFirstName" HorizontalAlignment="Left" Height="27" Margin="340,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" 
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblMiddleName" Content="Middle Name: " HorizontalAlignment="Left" Margin="179,199,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxMiddleName" HorizontalAlignment="Left" Height="27" Margin="340,204,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" 
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblLastName" Content="Last Name: " HorizontalAlignment="Left" Margin="179,235,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxLastName" HorizontalAlignment="Left" Height="27" Margin="340,244,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblGender" Content="Gender: " HorizontalAlignment="Left" Margin="179,276,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <RadioButton Content="Male" HorizontalAlignment="Left" Margin="340,280,0,0" VerticalAlignment="Top" Width="72" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5"/>
        <RadioButton Content="Female" HorizontalAlignment="Left" Margin="417,280,0,0" VerticalAlignment="Top" Width="87" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblBirthDate" Content="Birth Date: " HorizontalAlignment="Left" Margin="179,312,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <DatePicker HorizontalAlignment="Left" Margin="340,320,0,0" VerticalAlignment="Top" Width="151"/>
        <Label x:Name="lblPin" Content="Pin: " HorizontalAlignment="Left" Margin="179,348,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxPin" HorizontalAlignment="Left" Height="27" Margin="340,357,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"/>
        <Label x:Name="lblContacts" Content="Contacts: " HorizontalAlignment="Left" Margin="179,401,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblPhoneNumber" Content="Phone Number: " HorizontalAlignment="Left" Margin="179,438,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCountryCode" HorizontalAlignment="Left" Height="27" Margin="340,444,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="151"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"  Text="08XXXXXXXXX"/>
        <Label x:Name="lblEmail" Content="E-mail: " HorizontalAlignment="Left" Margin="179,474,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="27" Margin="340,483,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1"  Text="email@example.com"/>
        <Label x:Name="lblAddress" Content="Address: " HorizontalAlignment="Left" Margin="179,538,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCountry" HorizontalAlignment="Left" Margin="340,583,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Label x:Name="lblCountry" Content="Country: " HorizontalAlignment="Left" Margin="179,574,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblCity" Content="City: " HorizontalAlignment="Left" Margin="179,610,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblStreet" Content="Street: " HorizontalAlignment="Left" Margin="179,646,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxCity" HorizontalAlignment="Left" Margin="340,619,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <TextBox x:Name="txtBoxStreet" HorizontalAlignment="Left" Margin="340,655,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Label x:Name="lblPersonalData" Content="Personal Data: " HorizontalAlignment="Left" Margin="179,117,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <Label x:Name="lblSubject" Content="Subject: " HorizontalAlignment="Left" Margin="179,708,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5"/>
        <TextBox x:Name="txtBoxSubject" HorizontalAlignment="Left" Margin="340,717,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top"/>
        <Label x:Name="lblMainTeacher" Content="Is Main Teacher?" HorizontalAlignment="Left" Margin="179,781,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5" RenderTransformOrigin="0.571,1.389"/>
        <RadioButton Content="Yes" HorizontalAlignment="Left" Margin="340,785,0,0" VerticalAlignment="Top" Width="72" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5" Checked="RadioButton_Checked_1"/>
        <RadioButton Content="No" HorizontalAlignment="Left" Margin="419,785,0,0" VerticalAlignment="Top" Width="72" Height="24"
    		FontSize="20" Foreground="#FFD8D5D5" Checked="RadioButton_Checked_2"/>
        <Label x:Name="lblSchoolClass" Content="School Class: " HorizontalAlignment="Left" Margin="179,835,0,0" VerticalAlignment="Top" Width="156" 
    		FontFamily="Sylfaen" FontSize="20" Foreground="#FFD8D5D5" Visibility="Hidden" />
        <TextBox x:Name="txtBoxSchoolClass" HorizontalAlignment="Left" Margin="340,844,0,0" TextWrapping="Wrap" Width="240"
    		FontFamily="Sylfaen" FontSize="16" UndoLimit="10" MaxLength="10" MaxLines="1" Height="27" VerticalAlignment="Top" Visibility="Hidden"/>

        <Button x:Name="btnCancel" Content="Cancel" Margin="417,917,213,0" VerticalAlignment="Top" Height="32" FontFamily="Sylfaen" FontSize="18" Foreground="#FF172949" Click="btnCancel_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.502"/>
                    <GradientStop Color="#FF04244A" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Button x:Name="btnSave" Content="Save" HorizontalAlignment="Left" Margin="220,917,0,0" VerticalAlignment="Top" Width="137" Height="32" FontFamily="Sylfaen" FontSize="18" Foreground="#FF172949" Click="btnSave_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF3F3F3" Offset="0"/>
                    <GradientStop Color="#FFEBEBEB" Offset="0.5"/>
                    <GradientStop Color="#FFDDDDDD" Offset="0.5"/>
                    <GradientStop Color="#FF04244A" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
    </Grid>
</Window>
